{
  "identifier": "desktops",
  "platforms": ["macOS", "windows", "linux"],
  "windows": ["main", "detail", "add", "schoice"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "core:window:allow-show",
    "core:window:allow-unminimize",
    "notification:allow-request-permission",
    "notification:default",
    "log:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**"
        },
        {
          "url": "http://**"
        }
      ]
    }
  ]
}
